COLUMN TABLE  "SAP_COMMON_CURRENCIES" ("NAME" NVARCHAR(255), "DESCR" NVARCHAR(1000), "CODE" NVARCHAR(3) NOT NULL,
 "SYMBOL" NVARCHAR(5), "NUMCODE" INTEGER, "EXPONENT" INTEGER, "MINOR" NVARCHAR(5000), PRIMARY KEY ("CODE")) WITH ASSOCIATIONS( JOIN  "SAP_COMMON_CURRENCIES_TEXTS" AS "LOCALIZED" ON localized.code = code AND localized.locale = SESSION_CONTEXT('LOCALE'), JOIN  "SAP_COMMON_CURRENCIES_TEXTS" AS "TEXTS" ON texts.code = code)  UNLOAD PRIORITY 5  AUTO MERGE 

