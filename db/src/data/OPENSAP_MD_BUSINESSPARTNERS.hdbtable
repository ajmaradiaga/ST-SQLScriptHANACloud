COLUMN TABLE  "OPENSAP_MD_BUSINESSPARTNERS" ("ID" NVARCHAR(36) NOT NULL,
 "CREATEDAT" LONGDATE, "CREATEDBY" NVARCHAR(255), "MODIFIEDAT" LONGDATE, "MODIFIEDBY" NVARCHAR(255), "PARTNERROLE" INTEGER DEFAULT 1, "EMAIL" NVARCHAR(255), "PHONENUMBER" NVARCHAR(30), "WEBADDRESS" NVARCHAR(1024), "COMPANYNAME" NVARCHAR(80), "LEGALFORM" NVARCHAR(10), "ADDRESS_ID" NVARCHAR(36), "CURRENCY_CODE" NVARCHAR(3), PRIMARY KEY ("ID")) WITH ASSOCIATIONS( JOIN  "OPENSAP_MD_ADDRESSES" AS "ADDRESS" ON address.ID = address_ID, JOIN  "OPENSAP_MD_EMPLOYEES" AS "CREATEDBYEMPLOYEE" ON createdByEmployee.email = createdBy, JOIN  "SAP_COMMON_CURRENCIES" AS "CURRENCY" ON currency.code = currency_code, JOIN  "OPENSAP_MD_EMPLOYEES" AS "MODIFIEDBYEMPLOYEE" ON modifiedByEmployee.email = modifiedBy)  UNLOAD PRIORITY 5  AUTO MERGE 

