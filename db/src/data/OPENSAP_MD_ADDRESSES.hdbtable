COLUMN TABLE  "OPENSAP_MD_ADDRESSES" ("ID" NVARCHAR(36) NOT NULL,
 "VALIDFROM" LONGDATE NOT NULL,
 "VALIDTO" LONGDATE, "CITY" NVARCHAR(40), "POSTALCODE" NVARCHAR(10), "STREET" NVARCHAR(60), "BUILDING" NVARCHAR(10), "REGION" NVARCHAR(4), "ADDRESSTYPE" NVARCHAR(2), "LATITUDE" DOUBLE, "LONGITUDE" DOUBLE, "COUNTRY_CODE" NVARCHAR(3), PRIMARY KEY INVERTED INDIVIDUAL("ID", "VALIDFROM")) WITH ASSOCIATIONS( JOIN  "SAP_COMMON_COUNTRIES" AS "COUNTRY" ON country.code = country_code)  UNLOAD PRIORITY 5  AUTO MERGE 

