COLUMN TABLE  "OPENSAP_MD_EMPLOYEES" ("ID" NVARCHAR(36) NOT NULL,
 "VALIDFROM" LONGDATE NOT NULL,
 "VALIDTO" LONGDATE, "NAMEFIRST" NVARCHAR(40), "NAMEMIDDLE" NVARCHAR(40), "NAMELAST" NVARCHAR(40), "NAMEINITIALS" NVARCHAR(10), "SEX" NVARCHAR(1), "LANGUAGE" NVARCHAR(1), "PHONENUMBER" NVARCHAR(30), "EMAIL" NVARCHAR(255), "LOGINNAME" NVARCHAR(12), "SALARYAMOUNT" DECIMAL(15,2), "ACCOUNTNUMBER" NVARCHAR(10), "BANKID" NVARCHAR(10), "BANKNAME" NVARCHAR(255), "EMPLOYEEPICURL" NVARCHAR(255), "ADDRESS_ID" NVARCHAR(36), "CURRENCY_CODE" NVARCHAR(3), PRIMARY KEY INVERTED INDIVIDUAL("ID", "VALIDFROM")) WITH ASSOCIATIONS( JOIN  "OPENSAP_MD_ADDRESSES" AS "ADDRESS" ON address.ID = address_ID, JOIN  "SAP_COMMON_CURRENCIES" AS "CURRENCY" ON currency.code = currency_code)  UNLOAD PRIORITY 5  AUTO MERGE 

