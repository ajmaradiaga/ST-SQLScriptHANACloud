COLUMN TABLE  "OPENSAP_PURCHASEORDER_ITEMS" ("ID" NVARCHAR(36) NOT NULL,
 "GROSSAMOUNT" DECIMAL(15,2), "NETAMOUNT" DECIMAL(15,2), "TAXAMOUNT" DECIMAL(15,2), "QUANTITY" DECIMAL(13,3), "QUANTITYUNIT" NVARCHAR(3), "NOTEID" NVARCHAR(10), "DELIVERYDATE" SECONDDATE, "CURRENCY_CODE" NVARCHAR(3), "POHEADER_ID" NVARCHAR(36), "PRODUCT_PRODUCTID" NVARCHAR(10), PRIMARY KEY ("ID")) WITH ASSOCIATIONS( JOIN  "SAP_COMMON_CURRENCIES" AS "CURRENCY" ON currency.code = currency_code, JOIN  "OPENSAP_PURCHASEORDER_HEADERS" AS "POHEADER" ON poHeader.ID = poHeader_ID, JOIN  "OPENSAP_MD_PRODUCTS" AS "PRODUCT" ON product.productId = product_productId)  UNLOAD PRIORITY 5  AUTO MERGE ;
