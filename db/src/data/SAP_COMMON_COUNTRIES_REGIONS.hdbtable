COLUMN TABLE  "SAP_COMMON_COUNTRIES_REGIONS" ("COUNTRY" NVARCHAR(3) NOT NULL,
 "SUB_CODE" NVARCHAR(5) NOT NULL,
 "NAME" NVARCHAR(80), "TYPE" NVARCHAR(80), PRIMARY KEY INVERTED INDIVIDUAL("COUNTRY", "SUB_CODE")) WITH ASSOCIATIONS( JOIN  "SAP_COMMON_COUNTRIES" AS "TOCOUNTRIES" ON toCountries.code = country)  UNLOAD PRIORITY 5  AUTO MERGE 

