COLUMN TABLE  "OPENSAP_MD_PRODUCTS" ("CREATEDAT" LONGDATE, "CREATEDBY" NVARCHAR(255), "MODIFIEDAT" LONGDATE, "MODIFIEDBY" NVARCHAR(255), "QUANTITY" DECIMAL(13,3), "QUANTITYUNIT" NVARCHAR(3), "PRODUCTID" NVARCHAR(10) NOT NULL,
 "TYPECODE" NVARCHAR(2), "CATEGORY" NVARCHAR(40), "NAME" NVARCHAR(5000), "DESC" NVARCHAR(5000), "WEIGHTMEASURE" DECIMAL(13,3), "WEIGHTUNIT" NVARCHAR(3), "PRICE" DECIMAL(15,2), "WIDTH" DECIMAL(13,3), "DEPTH" DECIMAL(13,3), "HEIGHT" DECIMAL(13,3), "DIMENSIONUNIT" NVARCHAR(3), "IMAGEURL" NVARCHAR(5000), "PARTNER_ID" NVARCHAR(36), "CURRENCY_CODE" NVARCHAR(3), "IMAGE_PRODUCT_PRODUCTID" NVARCHAR(10), PRIMARY KEY ("PRODUCTID")) WITH ASSOCIATIONS( JOIN  "OPENSAP_MD_EMPLOYEES" AS "CREATEDBYEMPLOYEE" ON createdByEmployee.email = createdBy, JOIN  "SAP_COMMON_CURRENCIES" AS "CURRENCY" ON currency.code = currency_code, JOIN  "OPENSAP_MD_PRODUCTIMAGES" AS "IMAGE" ON image.product_productId = image_product_productId, JOIN  "OPENSAP_MD_PRODUCTS_TEXTS" AS "LOCALIZED" ON localized.productId = productId AND localized.locale = SESSION_CONTEXT('LOCALE'), JOIN  "OPENSAP_MD_EMPLOYEES" AS "MODIFIEDBYEMPLOYEE" ON modifiedByEmployee.email = modifiedBy, JOIN  "OPENSAP_MD_BUSINESSPARTNERS" AS "PARTNER" ON partner.ID = partner_ID, JOIN  "OPENSAP_MD_PRODUCTS_TEXTS" AS "TEXTS" ON texts.productId = productId)  UNLOAD PRIORITY 5  AUTO MERGE ;


