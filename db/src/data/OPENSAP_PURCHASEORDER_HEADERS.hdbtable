COLUMN TABLE  "OPENSAP_PURCHASEORDER_HEADERS" ("CREATEDAT" LONGDATE, "CREATEDBY" NVARCHAR(255), "MODIFIEDAT" LONGDATE, "MODIFIEDBY" NVARCHAR(255), "ID" NVARCHAR(36) NOT NULL,
 "GROSSAMOUNT" DECIMAL(15,2), "NETAMOUNT" DECIMAL(15,2), "TAXAMOUNT" DECIMAL(15,2), "NOTEID" NVARCHAR(10), "LIFECYCLESTATUS" NVARCHAR(1) DEFAULT 'N', "APPROVALSTATUS" NVARCHAR(1), "CONFIRMSTATUS" NVARCHAR(1), "ORDERINGSTATUS" NVARCHAR(1), "INVOICINGSTATUS" NVARCHAR(1), "CURRENCY_CODE" NVARCHAR(3), "PARTNER_ID" NVARCHAR(36), PRIMARY KEY ("ID")) WITH ASSOCIATIONS( JOIN  "OPENSAP_MD_EMPLOYEES" AS "CREATEDBYEMPLOYEE" ON createdByEmployee.email = createdBy, JOIN  "SAP_COMMON_CURRENCIES" AS "CURRENCY" ON currency.code = currency_code, JOIN  "OPENSAP_PURCHASEORDER_ITEMS" AS "ITEM" ON item.poHeader_ID = ID, JOIN  "OPENSAP_MD_EMPLOYEES" AS "MODIFIEDBYEMPLOYEE" ON modifiedByEmployee.email = modifiedBy, JOIN  "OPENSAP_MD_BUSINESSPARTNERS" AS "PARTNER" ON partner.ID = partner_ID)  UNLOAD PRIORITY 5  AUTO MERGE ;



